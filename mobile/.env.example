# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:8000
EXPO_PUBLIC_WS_URL=ws://localhost:8001

# Environment
EXPO_PUBLIC_ENV=development

# Feature Flags
EXPO_PUBLIC_ENABLE_GPS_TRACKING=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true

# GPS Settings
EXPO_PUBLIC_GPS_UPDATE_INTERVAL=10000
EXPO_PUBLIC_GPS_DISTANCE_THRESHOLD=50

# Photo Settings
EXPO_PUBLIC_PHOTO_QUALITY=0.8
EXPO_PUBLIC_MAX_PHOTO_SIZE=5242880

# Debug
EXPO_PUBLIC_DEBUG_MODE=false
EXPO_PUBLIC_LOG_LEVEL=info
